# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'TestAE' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TestAE
# https://github.com/realm/SwiftLint
  pod 'SwiftLint'

# https://github.com/cruffenach/CRToast
  pod 'CRToast', '~> 0.0.9'

# https://github.com/hackiftekhar/IQKeyboardManager
  pod 'IQKeyboardManager'

# https://github.com/dani-gavrilov/GDPerformanceView-Swift
  pod 'GDPerformanceView-Swift', '~> 1.2.0'

# https://developers.google.com/maps/documentation/ios-sdk/start?hl=ru
  pod 'GoogleMaps'
  pod 'GooglePlaces'


  target 'TestAETests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'TestAEUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
